{
  "run.timelimit": 300000,
  "run.compiler": "mingw32-g++.exe -Wall -g -IC:MinGWinclude ",
  "run.compilerArgs": "-o Program.exe -L C:MinGWlib -lglut32 -lopengl32 -lglu32 -lwinmm -lgdi32",
  "C_Cpp.formatting": "vcFormat",
  "breadcrumbs.enabled": false,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "files.exclude": {
    "mingw*": true,
    "Program.exe": true
  },
  "files.associations": {
    "iostream": "cpp",
    "fstream": "cpp",
    "ostream": "cpp"
  }
}
